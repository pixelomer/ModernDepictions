{
	// See https://go.microsoft.com/fwlink/?LinkId=733558
	// for the documentation about the tasks.json format
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Clean build",
			"type": "shell",
			"command": "make clean all",
			"problemMatcher": [
				"$gcc"
			]
		},
		{
			"label": "Build tweak and install package",
			"type": "shell",
			"command": "make all package install",
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		}
	]
}